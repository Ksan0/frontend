<fest:template xmlns:fest="http://fest.mail.ru"
    context_name="json">
            <div class="content font_type">
                <div class="content__title">ARCANOID</div>
                <div class="content__main win__game"></div>
                <div class="content__main">Your Score: <fest:value>json.score</fest:value></div>
                <form id="gameOverForm">
                    <input type="text" class="scoreform " id="username" />
                    <input type="submit" class="content__button" />
                </form>
                <input class="content__button hide__gameover" type="button" value="Restart" />
                <a class="content__button hide__gameover" href="#main">Menu</a>
            </div>
</fest:template>